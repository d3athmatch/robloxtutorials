while true do
	if script.Parent:FindFirstChild("PizzaBox") == nil then
		
		local replicatedStorage = game:GetService("ReplicatedStorage")
		local hiddenBox = replicatedStorage:FindFirstChild("PizzaBox")
		local randTime = math.random(5, 30)
		
		wait(randTime)
		
		local boxClone = hiddenBox:Clone()
		boxClone.Parent = script.Parent
		
	end
	wait(1)
	
end
